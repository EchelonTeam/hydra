msgid "Be Verbose"
msgstr "Подробный вывод"

#: ../src/interface.c:369
msgid "be verbose"
msgstr "Подробный вывод консольной утилиты"

#: ../src/interface.c:371
msgid "Debug"
msgstr "Отладка"

#: ../src/interface.c:375
msgid "Enable debug mode"
msgstr "Включить режим отладки"

